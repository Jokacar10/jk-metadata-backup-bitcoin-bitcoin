      "html_url": "https://github.com/martinsaposnic",
      "followers_url": "https://api.github.com/users/martinsaposnic/followers",
      "following_url": "https://api.github.com/users/martinsaposnic/following%7B/other_user%7D",
      "gists_url": "https://api.github.com/users/martinsaposnic/gists%7B/gist_id%7D",
      "starred_url": "https://api.github.com/users/martinsaposnic/starred%7B/owner%7D%7B/repo%7D",
      "subscriptions_url": "https://api.github.com/users/martinsaposnic/subscriptions",
      "organizations_url": "https://api.github.com/users/martinsaposnic/orgs",
      "repos_url": "https://api.github.com/users/martinsaposnic/repos",
      "events_url": "https://api.github.com/users/martinsaposnic/events%7B/privacy%7D",
