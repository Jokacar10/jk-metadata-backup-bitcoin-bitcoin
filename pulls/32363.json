    "title": "ci: Use previous releases in tests on Windows",
    "user": {
      "login": "GarmashAlex",
      "id": 193699300,
      "node_id": "U_kgDOC4ud5A",
      "avatar_url": "https://avatars.githubusercontent.com/u/193699300?v=4",
