    "title": "rpc: Ensure -debug=0/none behaves consistently with -nodebug",
    "user": {
      "login": "danielabrozzoni",
      "id": 25042473,
      "node_id": "MDQ6VXNlcjI1MDQyNDcz",
      "avatar_url": "https://avatars.githubusercontent.com/u/25042473?v=4",
      "gravatar_id": "",
