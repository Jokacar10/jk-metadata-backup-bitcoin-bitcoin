      "gists_url": "https://api.github.com/users/tomt1664/gists%7B/gist_id%7D",
      "starred_url": "https://api.github.com/users/tomt1664/starred%7B/owner%7D%7B/repo%7D",
      "subscriptions_url": "https://api.github.com/users/tomt1664/subscriptions",
      "organizations_url": "https://api.github.com/users/tomt1664/orgs",
